{
  "name": "v-scroll-affix",
  "version": "0.0.2",
  "description": "A lightweight vue component for affixing an element as you scroll. 轻量级 vue 固钉组件。",
  "main": "index.js",
  "scripts": {
    "rm": "../../node_modules/.bin/rimraf dist",
    "dev": "npm run rm && ../../node_modules/.bin/cross-env NODE_ENV=development ../../node_modules/.bin/rollup -cw",
    "build": "npm run rm && ../../node_modules/.bin/cross-env NODE_ENV=production ../../node_modules/.bin/rollup -c"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shalldie/scroll-affix.git"
  },
  "keywords": [
    "vue",
    "scroll",
    "affix",
    "lib"
  ],
  "author": "shalldie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shalldie/scroll-affix/issues"
  },
  "homepage": "https://github.com/shalldie/scroll-affix#readme",
  "peerDependencies": {
    "vue": "*"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "rollup-plugin-vue": "^5.1.9",
    "scroll-affix": "^0.0.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
